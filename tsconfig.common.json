{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noLib": false,
    "outDir": "dist",
    "removeComments": true,
    "rootDir": "./",
    "sourceMap": true,
    "strict": true,
    "strictFunctionTypes": false,
    "skipLibCheck": false,
    "target": "ES6",
    "watch": false,
    "esModuleInterop":true
  },
  "exclude": [
    "node_modules/"
  ]
}
